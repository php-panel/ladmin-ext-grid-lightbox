{
    "name": "php-panel/ladmin-ext-grid-lightbox",
    "description": "Turn your grid into a lightbox & gallery",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "extension",
        "grid",
        "lightbox",
        "gallery"
    ],
    "homepage": "https://github.com/php-panel/ladmin-ext-grid-lightbox",
    "license": "MIT",
    "authors": [
        {
            "name": "leeqvip",
            "email": "leeqvip@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": ">=12.0",
        "php-panel/laravel-admin": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.0"
    },
    "autoload": {
        "psr-4": {
            "Ladmin\\Grid\\Lightbox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ladmin\\Grid\\Lightbox\\LightboxServiceProvider"
            ]
        }
    }
}